{
  "name": "greynoise-mcp-server",
  "version": "0.1.0",
  "description": "MCP Server for the GreyNoise Enterprise API",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "gnapi": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 build/index.js && cp .env build/.env",
    "start": "node build/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "mcp",
    "greynoise",
    "security",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
